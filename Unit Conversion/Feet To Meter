/**
 * Convert Feet to meter.
 */
let width=40;
let length=60;
let l_in_meter=length*3.281;
let w_in_meter=width*3.281;
console.log("Length in meter: ",l_in_meter,"meters");
console.log("Width in meter: ",w_in_meter,"meters");


============O/P=====================================


C:\Users\admin>node feet.js
Length in meter:  196.86 meters
Width in meter:  131.24 meters
